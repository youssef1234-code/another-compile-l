{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "event-manager-ai-service",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/index.py" }
  ],
  "functions": {
    "api/**/*.py": {
      "maxDuration": 60
    }
  }
}
